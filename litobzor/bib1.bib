   keywords = {Degradation,Electrodes,Hydrogen,Mechanism,Nafion membrane,Oxygen},
   month = {8},
   pages = {27956-27973},
   publisher = {Pergamon},
   title = {Nafion degradation mechanisms in proton exchange membrane fuel cell (PEMFC) system: A review},
   volume = {46},
   url = {https://linkinghub.elsevier.com/retrieve/pii/S0360319921021832},
   year = {2021},
